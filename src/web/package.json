{
  "name": "umbrella-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:perf": "jest --testMatch='**/*.perf.test.ts'",
    "test:a11y": "jest-axe",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "analyze": "webpack-bundle-analyzer stats.json",
    "security-audit": "npm audit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run analyze"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.0.0",
    "@opentelemetry/api": "latest",
    "@spring-boot/actuator-client": "^3.2.1",
    "@spring-boot/actuator-metrics": "^3.2.1",
    "@spring-boot/actuator-react": "^3.2.0",
    "@spring-boot/metrics": "^2.0.0",
    "@spring-boot/types": "latest",
    "axios": "^1.6.0",
    "date-fns": "2.30.0",
    "lodash": "^4.17.21",
    "numeral": "2.0.6",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-hook-form": "^7.0.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.0.0",
    "react-use": "17.4.0",
    "styled-components": "^6.1.0",
    "winston": "latest",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@analytics/react": "^0.1.0",
    "@axe-core/react": "^4.8.2",
    "@jest/globals": "^29.7.0",
    "@monitoring/react": "^1.0.0",
    "@spring-boot/actuator-hooks": "^3.2.1",
    "@spring-boot/actuator-mock": "^1.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/performance": "^1.0.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-spring-boot": "^1.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.7.0",
    "@types/monitoring": "^1.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/spring-boot": "^2.0.0",
    "@types/styled-components": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-styled-components": "^7.2.0",
    "mini-css-extract-plugin": "^2.7.0",
    "perf_hooks": "^1.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "terser-webpack-plugin": "^5.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.4",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}